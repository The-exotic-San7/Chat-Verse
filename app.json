{
  "expo": {
    "name": "chatverse",
    "slug": "mobile",
    "description": "Chat verse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/react-native-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/react-splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/react-native-icon.png"
        
      },
      "package": "com.san7.chatverse"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
